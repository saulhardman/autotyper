{
  "private": true,
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-env": "^1.1.7",
    "browser-env": "^2.0.19",
    "camel-case": "^3.0.0",
    "coveralls": "^2.11.15",
    "cross-env": "^3.1.4",
    "del": "^2.2.2",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-ava": "^4.0.0",
    "eslint-plugin-html": "^1.7.0",
    "eslint-plugin-import": "^2.2.0",
    "expose-loader": "^0.7.1",
    "gzip-size": "^3.0.0",
    "html-webpack-plugin": "^2.25.0",
    "jquery": "^3.1.1",
    "lerna": "^2.0.0-beta.32",
    "lodash.without": "^4.4.0",
    "npm-run-all": "^4.0.0",
    "nyc": "^10.1.2",
    "pretty-bytes": "^4.0.2",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "simple-git": "^1.65.0",
    "title-case": "^2.1.0",
    "webpack": "^2.2.0-rc.3",
    "webpack-dev-server": "^2.2.0-rc.0"
  },
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --inline",
    "test": "eslint . && cross-env NODE_ENV=test nyc ava",
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "commit": "node scripts/commit.js",
    "lerna-version": "lerna publish --skip-git --skip-npm --force-publish=*",
    "prepare-release": "npm-run-all build test lerna-version build commit"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-inline-environment-variables",
      "transform-object-rest-spread",
      "transform-object-assign"
    ]
  },
  "ava": {
    "require": [
      "babel-register",
      "./test/helpers/setup-browser-env.js"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "include": [
      "packages/**/src/*.js"
    ]
  }
}
