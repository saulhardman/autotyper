{
  "name": "autotyper",
  "description": "A simple JavaScript plugin that automatically types text.",
  "main": "index.js",
  "module": "index.es.js",
  "jsnext:main": "index.es.js",
  "browser": "index.umd.pkgd.min.js",
  "keywords": [
    "automatic",
    "auto",
    "type",
    "typer",
    "autotyper",
    "es2015",
    "es6",
    "es"
  ],
  "author": "Saul Hardman <hello@iamsaul.co.uk>",
  "homepage": "https://github.com/saulhardman/autotyper/",
  "version": "0.11.0",
  "repository": "https://github.com/saulhardman/autotyper.git",
  "dependencies": {
    "component-emitter": "^1.2.1"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ],
    "babelrc": false,
    "exclude": "node_modules/**",
    "runtimeHelpers": true
  },
  "ava": {
    "require": [
      "babel-register",
      "./test/helpers/setup-browser-env.js"
    ],
    "babel": "inherit"
  },
  "license": "MIT"
}